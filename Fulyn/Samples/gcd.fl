gcd :: [int => int => int]
a :: int

main
  a = gcd(1547, 2093)
end

gcd(x, y)
  tmp = x
  xg = x
  yg = y
  xg = ? les(xg, yg)
       | true -> yg
	   | false -> xg
  yg = ? eql(xg, tmp)
       | true -> yg
	   | false -> tmp
  ? mod(xg, yg)
  | 0 -> yg
  | () -> gcd(yg, mod(xg, yg))
end

