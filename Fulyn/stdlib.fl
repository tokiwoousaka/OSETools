add(x, y)
  inline: 14 30 {x} {y}
end 

sub(x, y)
  inline: 15 30 {x} {y}
end

mul(x, y)
  inline: 16 30 {x} {y}
end

div(x, y)
  inline: 1A 30 {x} {y}
end

mod(x, y)
  inline: 1B 30 {x} {y}
end

pow(x, y)
  xp = x
  yp = y
  ? gra(yp, 1)
  | true -> mul(xp, pow(xp, sub(yp, 1)))
  | false -> xp
end

eql(x, y)
  inline: 20 30 {x} {y}
end

