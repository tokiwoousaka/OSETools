end

xor(x, y)
  inline: 11 30 {x} {y}
end

sfl(x, y)
  inline: 18 30 {x} {y}
end

sfr(x, y)
  inline: 19 30 {x} {y}
end

for(x, comp, next, exec)
  xf = x
  cmpf = comp
  nxtf = next
  excf = exec
  flag = comp(xf)
  ? comp(xf)
  | true -> for2(xf, cmpf, nxtf, excf)
  | false -> 0
end

for2(x, comp, next, exec)
  xf = x
  cmpf = comp
  nxtf = next
  excf = exec
  excf(xf)
  for(nxtf(xf), cmpf, nxtf, excf)
end

